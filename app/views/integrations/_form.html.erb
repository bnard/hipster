<%= simple_form_for([@user, @integration]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :harvest_project_id %>
    <%= f.input :pivotal_project_id %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
